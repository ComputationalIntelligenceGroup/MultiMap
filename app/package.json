{
  "name": "multimap",
  "author": "Gherardo Varando <gherardo.varando@gmail.com>",
  "version": "1.3.0",
  "description": "An extendible and flexible application for maps analysis and visualization",
  "product name": "MultiMap",
  "homepage": "https://github.com/gherardovarando/Atlas",
  "bugs": {
    "email": "gherardo.varando@gmail.com"
  },
  "main": "main.js",
  "scripts": {
    "startclean": "electron . --clean --enable-logging",
    "start": "electron . --enable-logging",
    "pack:osx": "electron-packager .   --overwrite --version=1.4.5 --platform=darwin --arch=x64 --icon=icon.icns",
    "pack:win32": "electron-packager . --overwrite --version=1.4.5 --platform=win32 --arch=ia32 --icon=icon.ico",
    "pack:win64": "electron-packager . --overwrite --version=1.4.5 --platform=win32 --arch=x64  --icon=icon.ico",
    "pack:linux": "electron-packager . --overwrite --version=1.4.5 --platform=linux --arch=x64  --icon=icon.png",
    "packall": "npm run pack:osx && npm run pack:win32 && npm run pack:win64 && npm run pack:linux"
  },
  "repository": "https://github.com/gherardovarando/Atlas",
  "keywords": [
    "MultiMap",
    "Atlas",
    "map",
    "leaflet",
    "brain",
    "neuroscience",
    "spatial data",
    "electrongui",
    "electron"
  ],
  "license": "MIT",
  "devDependencies": {
    "electron": "2.0.0",
    "electron-packager": "*"
  },
  "dependencies": {
    "electron-is-dev": "^0.1.2",
    "electrongui": "^1.1.0"
  }
}
